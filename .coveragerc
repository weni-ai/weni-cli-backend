[run]
source = app
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */.pytest_cache/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    
[path]
source =
    ./app 